{
  "name": "@onozaty/pukiwiki-to-md",
  "version": "1.3.0",
  "description": "A converter from PukiWiki to Markdown",
  "type": "module",
  "bin": {
    "pukiwiki-to-md": "./dist/index.mjs"
  },
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build": "tsdown",
    "prepublishOnly": "pnpm run build",
    "dev": "tsx src/index.ts",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit",
    "check": "pnpm run lint && pnpm run typecheck",
    "format": "prettier --write src/",
    "test": "vitest run",
    "test:cov": "vitest run --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/onozaty/pukiwiki-to-md.git"
  },
  "keywords": [
    "pukiwiki",
    "markdown",
    "converter"
  ],
  "author": "onozaty",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/onozaty/pukiwiki-to-md/issues"
  },
  "homepage": "https://github.com/onozaty/pukiwiki-to-md#readme",
  "packageManager": "pnpm@10.12.1",
  "devDependencies": {
    "@eslint/js": "^9.37.0",
    "@types/node": "^24.7.2",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/eslint-plugin": "^1.3.23",
    "eslint": "^9.37.0",
    "globals": "^16.4.0",
    "jiti": "^2.6.1",
    "prettier": "^3.6.2",
    "tsdown": "^0.15.9",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.1",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "commander": "^14.0.1",
    "iconv-lite": "^0.7.0"
  }
}